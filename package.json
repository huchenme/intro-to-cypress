{
  "name": "calculator",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci test:e2e:run test:e2e:dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-test start 8080 cy:run",
    "test:e2e:dev": "start-test dev 8080 cy:open",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "node server",
    "dev:client": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "postbuild": "cp ./public/index.html ./dist/index.html",
    "start": "npm-run-all --parallel start:*",
    "start:server": "node server",
    "start:client": "serve --no-clipboard --single --listen 8080 dist",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|css|html|md)\"",
    "lint": "jest --config test/jest.lint.js",
    "validate": "npm run test:e2e:run",
    "postinstall": "cd server && npm install",
    "setup": "npm install && npm run validate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:e2e:run"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|css|html|md)": [
      "prettier",
      "jest --findRelatedTests"
    ]
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@babel/runtime": "^7.10.1",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "cypress": "^4.7.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.1",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "lint-staged": "^10.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "serve": "^11.3.0",
    "start-server-and-test": "^1.10.8",
    "style-loader": "^1.1.3",
    "test-data-bot": "^0.8.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@reach/router": "^1.3.1",
    "axios": "^0.19.2",
    "emotion-theming": "^10.0.27",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
